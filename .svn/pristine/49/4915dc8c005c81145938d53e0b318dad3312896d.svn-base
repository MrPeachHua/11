body{
    background-color: #eeeeee;
    font-family: "Microsoft YaHei", "微软雅黑", "sans-serif";
}
input,button,select,textarea{
    outline:none;
}
table.editTable{
    margin-top: 20px;
}
table.editTable div{
     text-align: right;
 }
.selectList{
    width: 161px;
    border-radius: 4px;
    height: 25px;
    border: 1px solid #A2A2A2;
    margin-right: 19px;
}
table.editTable tr td{
    height: 40px;
    background-color: #FFF;
}
table.editTable .editHeaderTd{
    height: 50px;
    background-color: #ecf6ff;
}
.btn_blue{
    width: 80px;
    height: 30px;
    background-color: #39d5b8;
    color: #FFF;
    border: none;
    border-radius: 4px;
    outline: none;
    cursor: pointer;
    letter-spacing: 5px;
    font-weight: bold;
    text-align: center;
}
.bottom_btn div{
    margin-right:180px;
    margin-bottom: 20px;
    margin-top: 20px;
}
.content {
    display: none;
    position: absolute;
    top: 10%;
    left: 12%;
    width: 80%;
    height: 70%;
    /* border: 1px solid black; */
    background-color: white;
    z-index: 1002;
    overflow: auto;
}
#bg{
    background-color:#666;
    position:absolute;
    z-index:99;
    left:0;
    top:0;
    display:none;
    width:100%;
    height:100%;
    opacity:0.001;
    filter: alpha(opacity=50);
}
