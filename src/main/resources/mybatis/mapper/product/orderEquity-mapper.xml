	<!-- 根据主键ID查询 boxiang 2016-01-03 -->
	<select id="queryById" resultMap="resultMap">
		SELECT <include refid="columns" />
		  FROM T_ORDER_EQUITY 
	     WHERE ID = #{id}     
	</select>
<!-- 根据主键ID查询 boxiang 2016-01-03 -->
	<select id="queryByOrderId" resultMap="resultMap">
		SELECT <include refid="columns" />
		  FROM T_ORDER_EQUITY 
	     WHERE order_id = #{orderId}     
	</select>

	<!-- 查询列表 boxiang 2016-01-03 -->
	<select id="selectList" resultMap="resultMap">
		SELECT <include refid="columns" />
		  FROM T_ORDER_EQUITY
		 WHERE 1 = 1	
		 <include refid="whereSQL" />
	</select>

	<!-- 查询分页列表 boxiang 2016-01-03 -->
	<select id="queryListPage" resultMap="resultMap">
		SELECT <include refid="columns" />
		  FROM T_ORDER_EQUITY
		 WHERE 1 = 1
		 <include refid="wherePageSql" />
	</select>
	
		<select id="queryListPageForEquity" resultMap="resultMap1">
		SELECT tpp.management_fee_monthly_unit AS PRICE,oe.ORDER_ID,omi.ORDER_TYPE,omi.ORDER_STATUS,oe.CAR_NUMBER,oe.PARKING_ID,oe.BEGIN_DATE,oe.END_DATE,tp.PARKING_NAME,omi.AMOUNT_PAID,omi.AMOUNT_PAYABLE,omi.AMOUNT_DISCOUNT,oe.CUSTOMER,oe.MONTH_NUM 
		FROM t_order_equity oe 
		LEFT JOIN t_propertyparkinginfo tpp ON oe.PARKING_ID = tpp.village_id AND tpp.CAR_NUMBER = oe.CAR_NUMBER
		LEFT JOIN t_order_main omi ON omi.ORDER_ID = oe.ORDER_ID
		LEFT JOIN t_parking tp ON tp.parking_id = oe.PARKING_ID
		WHERE 1=1
		 <include refid="wherePageSql" />
		 GROUP BY oe.ORDER_ID
	</select>
	<!-- 插入 boxiang 2016-01-03 -->
	<insert id="insert" parameterType="com.boxiang.share.product.order.po.OrderEquity">
        INSERT INTO T_ORDER_EQUITY (
        	ID,
        	PARKING_ID,
        	ORDER_ID,
        	CAR_NUMBER,
        	PRICE,
        	BEGIN_DATE,
        	END_DATE,
        	MONTH_NUM,
        	CUSTOMER,
        	PARKING_NO,
        	IS_USED,
        	CREATEOR,
        	MODIFIED,
        	CREATE_DATE,
        	MODIFY_DATE
        ) VALUES (
        	#{id},
        	#{parkingId},
        	#{orderId},
        	#{carNumber},
        	#{price},
        	#{beginDate},
        	#{endDate},
        	#{monthNum},
        	#{customer},
        	#{parkingNo},
        	#{isUsed},
        	#{createor},
        	#{modified},
        	#{createDate},
        	#{modifyDate}
        )
	</insert>

