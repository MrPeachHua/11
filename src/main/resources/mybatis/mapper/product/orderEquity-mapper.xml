	</delete>

	<!-- 更新 boxiang 2016-01-03 -->
	<update id="update" parameterType="com.boxiang.share.product.order.po.OrderEquity">
        UPDATE T_ORDER_EQUITY 
		SET
	        PARKING_ID = #{parkingId},
	        ORDER_ID = #{orderId},
	        CAR_NUMBER = #{carNumber},
	        PRICE = #{price},
	        BEGIN_DATE = #{beginDate},
	        END_DATE = #{endDate},
	        MONTH_NUM = #{monthNum},
	        CUSTOMER = #{customer},
	        PARKING_NO = #{parkingNo},
	        IS_USED = #{isUsed},
	        CREATEOR = #{createor},
	        MODIFIED = #{modified},
	        CREATE_DATE = #{createDate},
	        MODIFY_DATE = #{modifyDate}
        WHERE ID = #{id} 	        
	</update>

	<update id="batchUpdate">
		<foreach collection="list" item="item" open="" close="" separator=";">
			UPDATE T_ORDER_EQUITY
			<set>
    	        PARKING_ID = #{item.parkingId},
    	        ORDER_ID = #{item.orderId},
    	        CAR_NUMBER = #{item.carNumber},
    	        PRICE = #{item.price},
    	        BEGIN_DATE = #{item.beginDate},
    	        END_DATE = #{item.endDate},
    	        MONTH_NUM = #{item.monthNum},
    	        CUSTOMER = #{item.customer},
    	        PARKING_NO = #{item.parkingNo},
    	        IS_USED = #{item.isUsed},
    	        CREATEOR = #{item.createor},
    	        MODIFIED = #{item.modified},
    	        CREATE_DATE = #{item.createDate},
    	        MODIFY_DATE = #{item.modifyDate}
			</set>
            WHERE ID = #{item.id} 	
		</foreach>
	</update>
